<div style="width: 100%;height:30px;">
  <h2 style="text-align: center;color:white"><b>ALL</b></h2>
</div>
<div class="container" style="margin-top: 20px;">
<div class="foods">
    <span><b>Reported Desaster</b></span>
  </div>
  <br><br>
  <div class="row">
    {{#each product}}
    <div class="col-sm-3" style="margin-left: 0px; height: 500px;">
      <div class="card" style="width: 15rem; height:78%;box-shadow: rgb(38, 57, 77) 0px 20px 30px -10px;">
  <img style="height: 200px;" src="/images/product/{{this.photo}}" class="card-img-top" alt="...">
  <div class="card-body">
    <h6 class="card-title"><b></b><b>{{this.name}}</b></h6>
    <h6 class="card-title">{{this.Product_id}}</h6>
    <h5 class="card-title">ReportedBy:{{this.clinicName}}</h5>
    <p class="card-text">{{this.Description}}</p>
    <p class="card-text"><b>{{this.Price}}</b></p>
    <a href="/https://anazks123.github.io/dis/live_maps.html" class="btn btn-outline-info">Check Live status</a>  
  </div>
</div>
    </div>
    {{/each}}
  </div>
</div>

